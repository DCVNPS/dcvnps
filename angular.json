{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-dcvnps": {
      "root": "src",
      "sourceRoot": "src/client",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "./src/server/public",
            "index": "src/client/index.html",
            "main": "src/client/main.ts",
            "tsConfig": "src/client/tsconfig.app.json",
            "polyfills": "src/client/polyfills.ts",
            "assets": [
              "src/client/assets",
              "src/client/favicon.ico",
              { "glob": "**/*", "input": "node_modules/tinymce/skins", "output": "/skins/" },
              { "glob": "**/*", "input": "node_modules/tinymce/themes", "output": "/themes/" },
              { "glob": "**/*", "input": "node_modules/tinymce/plugins", "output": "/plugins/" }
            ],
            "styles": [
              "src/client/styles.scss",
              "src/client/assets/semantic.min.css",
              "src/client/assets/semantic.grid.min.css"
            ],
            "scripts": [
              "src/client/assets/jquery-3.1.1.min.js",
              "src/client/assets/semantic.min.js",
              "node_modules/tinymce/tinymce.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/client/environments/environment.ts",
                  "with": "src/client/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-dcvnps:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-dcvnps:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-dcvnps:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/client/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/client/polyfills.ts",
            "tsConfig": "src/client/tsconfig.spec.json",
            "assets": [
              "src/client/assets",
              "src/client/favicon.ico",
              { "glob": "**/*", "input": "node_modules/tinymce/skins", "output": "/skins/" },
              { "glob": "**/*", "input": "node_modules/tinymce/themes", "output": "/themes/" },
              { "glob": "**/*", "input": "node_modules/tinymce/plugins", "output": "/plugins/" } 
            ],
            "styles": [
              "src/client/styles.scss",
              "src/client/assets/semantic.min.css",
              "src/client/assets/semantic.grid.min.css"
            ],
            "scripts": [
              "src/client/assets/jquery-3.1.1.min.js",
              "src/client/assets/semantic.min.js",
              "node_modules/tinymce/tinymce.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "ng-dcvnps-e2e": {
      "root": "src/e2e",
      "sourceRoot": "src/e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "ng-dcvnps:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "ng-dcvnps",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}