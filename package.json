{
  "name": "dcvnps",
  "version": "0.0.0",
  "scripts": {
    "tsc": "tsc",
    "ng": "ng",
    "clean": "rmdir /Q /S dist\\server",
    "mklogdir": "mkdir dist\\server\\logs",
    "prebuild": "npm run clean",
    "postbuild": "npm run mklogdir",
    "build:client": "ng build --prod",
    "build:server": "tsc",
    "build": "run-s build:server build:client",
    "watch:client": "ng serve --proxy-config proxy.conf.json  --open",
    "watch:server": "node projects/vnps-node/src/server/bin/www.ts",
    "watch": "run-p watch:*",
    "start:server": "node dist/server/bin/www.js",
    "start": "run-s build start:server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@tinymce/tinymce-angular": "^3.5.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-jwt": "^5.3.1",
    "express-request-id": "^1.4.1",
    "font-awesome": "^4.7.0",
    "knex": "^0.20.10",
    "mysql2": "^2.1.0",
    "rxjs": "~6.5.4",
    "tinymce": "^5.1.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.0.2",
    "protractor": "^6.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
